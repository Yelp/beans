{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VARIANT": "ubuntu-22.04" }
	},

	"forwardPorts": [5000, 8080],

	"postCreateCommand": "python .devcontainer/configs_in_env.py load_from_env",

	"remoteUser": "vscode",
	"features": {
		"sshd": "latest",
		"node": "20",
		"python": "3.11"
	}
}
